{
  "name": "cybershop-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "security-check": "npm audit && npm audit fix",
    "compatibility-check": "npx webhint https://localhost:5173",
    "performance-check": "npx lighthouse https://localhost:5173 --output=json --output-path=./lighthouse-report.json",
    "accessibility-check": "npx axe-core --version && echo 'Run axe-core in browser console for accessibility testing'",
    "test-all": "npm run security-check && npm run type-check && npm run lint",
    "build-prod": "NODE_ENV=production npm run build",
    "analyze": "npx vite-bundle-analyzer dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@mediapipe/camera_utils": "^0.3.1675466862",
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "@types/three": "^0.177.0",
    "axios": "^1.10.0",
    "echarts": "^5.6.0",
    "element-plus": "^2.3.8",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.3",
    "three": "^0.177.0",
    "vue": "^3.3.4",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@cypress/vue": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.19.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^13.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "happy-dom": "^12.10.3",
    "jsdom": "^22.1.0",
    "terser": "^5.17.6",
    "typescript": "~5.1.6",
    "vite": "^4.4.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
